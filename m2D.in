sbpType = mfc
kspTol = 1e-10


# Maxwell properties
viscDistribution = layered
viscVals = [5e12 5e12] # GPa s
viscDepths = [0 30]

#viscDistribution = effectiveVisc
#strainRate = 1e-12


timeIntegrator = RK32
timeControlType = P
stride1D = 10
stride2D = 10
maxStepCount = 1e8
initTime = 1e10      # s 7e9
maxTime = 1e16      # s
initDeltaT = 1     # s
minDeltaT = 1e-3     # s
maxDeltaT = 1e8      # s
atol = 1e-8
timeIntInds = [2 3]

rootTol = 1e-9

