Lz = 30 # km

DcVals = [16e-3 16e-3] # m
#DcVals = [0.0472 0.0472] # m
DcDepths = [0 30] # m


aVals = [0.015 0.015] # usual values
aDepths = [0 30] # usual values
bVals = [0.02 0.02 0.0 0.0] # usual values
bDepths = [0 12 15 30] # usual values
