timeIntegrator = IMEX
timeControlType = P
stride1D = 10 # how many steps between writing out 1D fields
stride2D = 100 # how many steps between writing out 2D fields
maxStepCount = 1e8
initTime = 1e11       # s
maxTime = 1e13      # s
minDeltaT = 1e-3     # s
maxDeltaT = 1e6      # s
atol = 1e-7

