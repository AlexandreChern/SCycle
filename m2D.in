sigmaNVals = [50 50] # MPa
#sigmaNVals = [0.1 588] # MPa
sigmaNDepths = [0 30]

vL = 1e-9    # m/s

inputDir = inputData/

shearDistribution = constant
{
problem = symmetric
muPlus = 30 # GPa
rhoPlus = 3.0 # g/cm^3
}

# Maxwell properties
viscDistribution = layered
viscVals = [1e13 1e13] # GPa s
viscDepths = [0 30]

#viscDistribution = effectiveVisc
#sigmadevVals = [35 35]
