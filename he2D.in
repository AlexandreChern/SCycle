#outputDir = /scratch/kallison/he/le_wfh_VwT_fw0.12_
#outputDir = /scratch/kallison/he/pl_wfh_wsh_fw0.12_
#outputDir = /scratch/kallison/he/pl_wfh_wsh_VwT_fw0.0_
#outputDir = /scratch/kallison/he/pl_wfh_wsh_VwT_fw0.12_
#outputDir = /scratch/kallison/he/pl_wfh_wsh_fw0.0_

#outputDir = /scratch/kallison/he/pl_Dc1_wfh_wsh_VwT_fw0.0_
#outputDir = /scratch/kallison/he/plSS_Dc1_nfh_wsh_VwT_fw0.0_


outputDir = /scratch/kallison/he/test_


