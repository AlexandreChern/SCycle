% load and visualize data from C++ eqCycle code when run with 1D (Nz==1)
clear all
clc


sourceDir = '/Users/kallison/eqcycle/data/ex1_';
d = loadContext(sourceDir);
d = loadData1D(d,sourceDir,1);
d = loadData2D(d,sourceDir,1);
fprintf('min dy = %.3e m, max dy = %.3e m\n',min(d.dom.dy),max(d.dom.dy))

%%
fI = 1;

% plot simulation
figure(fI),clf,fI=fI+1;
subplot(2,1,1)
plot(D{ind}.time,D{ind}.tauP,'b.-','Linewidth',1)
ylabel('tau (MPa)')

subplot(2,1,2)
semilogy(D{ind}.time,D{ind}.slipVel,'b.-','Linewidth',1)
ylabel('slip velocity')

figure(fI),clf,fI=fI+1;
subplot(2,1,1)
plot(D{ind}.tauP,'b.-',,'Linewidth',1)
ylabel('tau (MPa)')

subplot(2,1,2)
semilogy(D{ind}.slipVel,'b.-',,'Linewidth',1)
ylabel('slip velocity')






