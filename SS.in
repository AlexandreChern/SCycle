#outputDir = data/lSS_uChol_TAMG_
#outputDir = data/lSS_uAMG_TAMG_
#outputDir = data/lSS_uChol_
#outputDir = data/lSS_uAMG1e-13_
outputDir = /scratch/kallison/testCo/SS_Co5_
