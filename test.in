order = 2
Ny = 5
Nz = 6
Ly = 4 # km
Lz = 5 # km

DcVals = [8e-3 8e-3] # m
DcDepths = [0 48] # m

bcT = traction
bcR = displacement
bcB = traction
bcL = displacement


aVals = [0.015 0.015] # comment
aDepths = [0 48]
bVals = [0.02 0.02 0.0 0.0]
