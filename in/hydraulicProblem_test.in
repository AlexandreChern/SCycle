order = 4 # SBP order of accuracy
Ny = 11
Nz = 12
Ly = 30 # (km) horizontal domain size or 60 km is fine
Lz = 30 # (km) vertical domain size
vL = 1e-9    # (m/s) loading velocity

# yes or no
isMMS = yes

# symmetric or general
geometry = symmetric

# linearElastic or powerLaw
bulkDeformationType = linearElastic

# coordinate transformation
bCoordTrans = 10.0
sbpType = mfc_coordTrans
#sbpType = mfc

# quasidynamic, dynamic, quasidynamic_and_dynamic, steadyStateIts
problemType = quasidynamic

#======================================================================
# linear system
#inputDir = /data/dunham/kallison/eqcycle/inputData/he/pl_al_
loadICs = 0
bcLTauQS = 0

# algorithm for momentum balance equation
linSolver = MUMPSCHOLESKY
#kspTol = 1e-10

# algorithm for heat equation
linSolver_heateq = AMG
kspTol_heateq = 1e-9

# algorithm for hydraulic problem (1D on fault)
linSolver_p = AMG
kspTol_p = 1e-10

#======================================================================
# rate-and-state parameters
rootTol = 1e-9

stateLaw = agingLaw
v0 = 1e-6
f0 = 0.6

impedanceVals = [9.4868 9.4868]
impedanceDepths = [0 60]
cohesionVals = [0 0] # m
cohesionDepths = [0 60] # m
#DcVals = [32e-3 32e-3] # m
DcVals = [16e-3 16e-3] # m or try 16-3 mfor an easier to resolve problem
DcDepths = [0 60] # m

## a,b for geotherm of 30 C/km, from Blanpied 1991 for a-b, 1995 for a
#aVals = [0.0135 0.0300 0.0700 0.2652]
#aDepths = [0 11.0000 19.3333 60]
#bVals = [0.0230 0.0379 0.0350 0.0375 0.0497]
#bDepths = [0 10 11 19.3333 60]

# easier to resolve
aVals = [0.015 0.015]
aDepths = [0 60]
bVals = [0.02 0.02 0.0 0.0]
bDepths = [0 12 15 60]

# normal stress
sigmaNVals = [50 50] #(MPa)
sigmaNDepths = [0 60] #km

# effective normal stress
#sigmaNVals = [5.668 5.668 1336] # MPa
#sigmaNDepths = [0 0.25 60]
#sigmaN_cap = 50; # MPa


#======================================================================
# hydraulic fault inputs
#hydraulicCoupling = no
hydraulicCoupling = uncoupled
#hydraulicCoupling = coupled

#hydraulicTimeIntType = explicit
hydraulicTimeIntType = implicit

# actual, physical values
n_pVals = [0.1 0.1]
n_pDepths = [0 30]
beta_pVals = [10 10] # 1/gPa
beta_pDepths = [0 30]
k_pVals = [1e-19 1e-19] # km^2
k_pDepths = [0 30]
eta_pVals = [1e-12 1e-12] # GPa s
eta_pDepths = [0 30]
g = 0
rho_fVals = [1 1] # g/cm^3
rho_fDepths = [0 30]
pVals = [0.0 0.0]
pDepths = [0 30]


#n_pVals = [1 1]
#n_pDepths = [0 30]
#beta_pVals = [1 1] # 1/MPa
#beta_pDepths = [0 30]
#k_pVals = [1 1] # km^2
#k_pDepths = [0 30]
#eta_pVals = [1 1] # MPa s
#eta_pDepths = [0 30]
#g = 0
#rho_fVals = [1 1] # g/cm^3
#rho_fDepths = [0 30]
#pVals = [0.0 0.0]
#pDepths = [0 30]

#======================================================================
# off-fault material parameters

# shear modulus distribution
shearDistribution = constant
{
problem = symmetric
muPlus = 30 # GPa
rhoPlus = 3.0 # g/cm^3
}

# heat equation properties
heatFieldsDistribution = layered
kVals = [1.89e-9 1.89e-9]  # crustal conductivity (km^2 kPa/K/s)
kDepths = [0 60]
hVals = [0 0]
hDepths = [0 60]
cVals = [900 900] # in m^2/s^2/K = J/g/K,  for granite, J/K/g
cDepths = [0 60]
rhoVals = [3 3] # g/cm^3
rhoDepths = [0 60]

TVals = [293 1793] # (K) geotherm corresponding to 30 C/km
TDepths = [0 50]
thermalCoupling = no
withShearHeating = no
withFrictionalHeating = no

#======================================================================
# settings for time integration # options: FEuler, RK32, RK43, RK32_WBE, RK43_WBE

timeIntegrator = RK32_WBE
timeControlType = P
stride1D = 10
stride2D = 10
maxStepCount = 1e8
initTime = 0
maxTime = 1e15      # (s)
initDeltaT = 1e-3   # (s) size of initial time step
minDeltaT = 1e-3    # (s) minimum allowed time step
maxDeltaT = 1e10    # (s) maximum allowed time step
atol = 1e-8 # absolute error for adaptive time stepper
timeIntInds = [psi slip]
#timeIntInds = [pressure]


outputDir = data/mms_




