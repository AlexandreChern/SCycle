#======================================================================
# settings for time integration # options: FEuler, RK32, RK43, RK32_WBE, RK43_WBE

timeIntegrator = RK43
stride1D = 50
stride2D = 50
maxStepCount = 1e8
initTime = 0
maxTime = 1e15
initDeltaT = 1e-3
minDeltaT = 1e-3
maxDeltaT = 1e11
atol = 1e-6
normType = L2_absolute
timeIntInds = [psi slip]
#timeIntInds = [psi slip gVxy gVxz]

outputDir = /scratch/kallison/paramSearch/v0a_





