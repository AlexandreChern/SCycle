order = 4 # SBP order of accuracy
Ny = 351
Nz = 302
Ly = 50 # (km) horizontal domain size
Lz = 50 # (km) vertical domain size
vL = 1e-9    # (m/s) loading velocity

# strikeSlip or iceStream
problemType = strikeSlip

# linearElastic or powerLaw
bulkDeformationType = linearElastic

# quasidynamic, dynamic, quasidynamic_and_dynamic, steadyStateIts
momentumBalanceType = quasidynamic

# whether or not to estimate steady state initial conditions
guessSteadyStateICs = 1

# coordinate transformation
bCoordTrans = 10.0
sbpType = mfc_coordTrans

#======================================================================
# linear system
#inputDir = /data/dunham/kallison/eqcycle/inputData/wfwo_Ny319_Nz201_
loadICs = 0

# algorithm for momentum balance equation: AMG or MUMPSCHOLESKY
linSolver = MUMPSCHOLESKY
kspTol = 1e-9

momBal_computeSxz = 1

#======================================================================
# rate-and-state parameters

#DcVals = [8e-3 8e-3] # m
DcVals = [12e-3 12e-3] # m
#DcVals = [1 1] # m
DcDepths = [0 60] # m

# creme brulee (30 C/km) from Blanpied 1991 for a-b, 1995 for a
aVals = [0.0135 0.0300 0.0700 0.2652]
aDepths = [0 11.0000 19.3333 60]
bVals = [0.0230 0.0379 0.0350 0.0375 0.0497]
bDepths = [0 10 11 19.3333 60]

# VS at surface
#aVals = [0.0135 0.0300 0.0700 0.2652]
#aDepths = [0 11.0000 19.3333 60]
#bVals = [0.0079 0.0242 0.0325 0.0301 0.0375 0.0737]
#bDepths = [0 4.5000 10 11.0000 19.3333 60]


# with varying degrees of overpressure
sNVals = [0 505.1] # lambda = 0.37
#sNVals = [0 401.1] # lambda = 0.5
#sNVals = [0 320.9] # lambda = 0.6
#sNVals = [0 240.6] # lambda = 0.7
#sNVals = [0 160.4] # lambda = 0.8
#sNVals = [50 50] # MPa
sNDepths = [0 40]
sN_floor = 5.0; # MPa
#sN_cap = 50; # MPa


# state variable law: agingLaw, slipLaw, flashHeating, constantState
stateLaw = agingLaw

#======================================================================
# off-fault material parameters

muVals = [32.4 32.4] # (GPa) shear modulus
muDepths = [0 60] # (km)
rhoVals = [2.7 2.7] # (g/cm^3) density
rhoDepths = [0 60] # (km)


# power law properties: quartz-diorite
#AVals = [1.3e-3 1.3e-3]
#ADepths = [0 60]
#BVals = [2.6341e+04 2.6341e+04]
#BDepths = [0 60]
#nVals = [2.4 2.4]
#nDepths = [0 60]

# power law properties: wet feldspar
AVals = [1585 1585]
ADepths = [0 60]
BVals = [4.157e4 4.157e4]
BDepths = [0 60]
nVals = [3 3]
nDepths = [0 60]

# heat equation properties
heatFieldsDistribution = layered
#kVals = [1.89e-9 1.89e-9]  # crustal conductivity (km^2 kPa/K/s)
kVals = [2.5e-9 2.5e-9]  # (km^2 kPa/K/s) crustal conductivity
kDepths = [0 60]
cVals = [900 900] # in m^2/s^2/K = J/g/K,  for granite, J/K/g
cDepths = [0 60]

# radioactive heat generation
he_A0Vals = [2.5e-9 2.5e-9 0 0] # (kW/m^3)
he_A0Depths = [0 30 30 60]
he_Lrad = 10 # km

# shear zone width
wVals = [1 1]
wDepths = [0 40]


# linear geotherms
#TVals = [293 1293] # 20 C/km
#TVals = [293 1543] # 25 C/km
#TVals = [293 1793] # 30 C/km
#TDepths = [0 50]

# varying LAB depth, potential temp of 1350 C, with mantle adiabat 0.3 C/km
#TVals = [283 1647] # LAB 60 km
#TDepths = [0 80]
#TVals = [283 1641] # LAB 60 km
#TDepths = [0 60]
TVals = [283 1638] # LAB 50 km
TDepths = [0 50]

thermalCoupling = coupled
heatEquationType = steadyState
withViscShearHeating = no
withFrictionalHeating = yes
withRadioHeatGeneration = yes

maxEffVisc = 1e18
maxSSIts_tau = 150

maxSSIts_effVisc = 100
#atolSS_effVisc = 5e-2
#gss_t = 1e-10
#fss_EffVisc = 0.2
#maxSSIts_effVisc = 3
#maxSSIts_tau = 5
maxSSIts_timesteps = 1e5


#======================================================================
# settings for time integration # options: FEuler, RK32, RK43, RK32_WBE, RK43_WBE

timeIntegrator = RK43_WBE
stride1D = 1
stride2D = 1
maxStepCount = 10
initTime = 0
maxTime = 1e15
initDeltaT = 1e-3
minDeltaT = 1e-3
maxDeltaT = 1e11
atol = 1e-6
normType = L2_absolute
timeIntInds = [psi slip]
#timeIntInds = [psi slip gVxy gVxz]

#outputDir = /scratch/kallison/versions/v7_
outputDir = /scratch/kallison/heat/le_w1_



