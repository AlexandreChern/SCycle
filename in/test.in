order = 4 # SBP order of accuracy
Ny = 101
Nz = 1
Ly = 30 # (km) horizontal domain size
Lz = 30 # (km) vertical domain size
vL = 1e-9    # (m/s) loading velocity

# strikeSlip or iceStream
problemType = strikeSlip

# linearElastic or powerLaw
bulkDeformationType = linearElastic

# quasidynamic, dynamic, quasidynamic_and_dynamic, steadyStateIts
momentumBalanceType = quasidynamic

# whether or not to estimate steady state initial conditions
guessSteadyStateICs = 1

# coordinate transformation
bCoordTrans = 6.0
sbpType = mfc_coordTrans
#sbpType = mfc

#======================================================================
# linear system
#inputDir = /data/dunham/kallison/eqcycle/inputData/he/pl_al_
loadICs = 0
bcLTauQS = 0

# algorithm for momentum balance equation
linSolver = MUMPSCHOLESKY
#kspTol = 1e-10

#======================================================================
# rate-and-state parameters
rootTol = 1e-9

stateLaw = agingLaw
v0 = 1e-9
f0 = 0.6

impedanceVals = [9.4868 9.4868]
impedanceDepths = [0 60]
cohesionVals = [0 0] # m
cohesionDepths = [0 60] # m
DcVals = [12e-3 12e-3] # m
#DcVals = [32e-3 32e-3] # m
#DcVals = [1 1] # m
DcDepths = [0 60] # m

# creme brulee (30 C/km) from Blanpied 1991 for a-b, 1995 for a
#aVals = [0.0135 0.0300 0.0700 0.2652]
#aDepths = [0 11.0000 19.3333 60]
#bVals = [0.0230 0.0379 0.0350 0.0375 0.0497]
#bDepths = [0 10 11 19.3333 60]

aVals = [0.015 0.051]
aDepths = [0 60]
bVals = [0.02 0.02]
bDepths = [0 60]

# effective normal stress
#sigmaNVals = [5.668 5.668 1336] # MPa
sigmaNVals = [50 50] # MPa
sigmaNDepths = [0 60]
#sigmaN_cap = 50; # MPa


#======================================================================
# fault inputs

# with varying degrees of overpressure
#sigmaNVals = [0 802.1] # lambda = 0.4
#sigmaNVals = [0 668.5] # lambda = 0.5
#sigmaNVals = [0 534.8] # lambda = 0.6
#sigmaNVals = [0 401.1] # lambda = 0.7
sigmaNVals = [0 267.4] # lambda = 0.8
sigmaNDepths = [0 60]
sigmaN_floor = 5.5; # MPa
#sigmaN_floor = 1; # MPa
#sigmaN_cap = 50; # MPa


# state variable law: agingLaw, slipLaw, flashHeating, constantState
stateLaw = agingLaw
v0 = 1e-9
f0 = 0.6

#======================================================================
# off-fault material parameters

shearModDistribution = constant
problem = symmetric
muPlus = 30 # GPa
rhoPlus = 3.0 # g/cm^3

# power law properties: quartz-diorite
#AVals = [1.3e-3 1.3e-3]
#ADepths = [0 60]
#BVals = [2.6341e+04 2.6341e+04]
#BDepths = [0 60]
#nVals = [2.4 2.4]
#nDepths = [0 60]

# power law properties: wet quartz
AVals = [1e-9 1e-9]
ADepths = [0 60]
BVals = [1.62e4 1.62e4]
BDepths = [0 60]
nVals = [4 4]
nDepths = [0 60]

# power law properties: wet feldspar
#AVals = [398 398]
#ADepths = [0 60]
#BVals = [4.28e4 4.28e4]
#BDepths = [0 60]
#nVals = [3 3]
#nDepths = [0 60]

# heat equation properties
heatFieldsDistribution = layered
kVals = [1.89e-9 1.89e-9]  # crustal conductivity (km^2 kPa/K/s)
kDepths = [0 60]
hVals = [0 0]
hDepths = [0 60]
cVals = [900 900] # in m^2/s^2/K = J/g/K,  for granite, J/K/g
cDepths = [0 60]
rhoVals = [3 3] # g/cm^3
rhoDepths = [0 60]

TVals = [293 1793] # (K) geotherm corresponding to 30 C/km
TDepths = [0 50]
thermalCoupling = no
withShearHeating = no
withFrictionalHeating = no

#======================================================================
# settings for time integration # options: FEuler, RK32, RK43, RK32_WBE, RK43_WBE

timeIntegrator = RK32
timeControlType = P
stride1D = 10
stride2D = 10
maxStepCount = 1e8
initTime = 0
maxTime = 1e11
initDeltaT = 1e-3
minDeltaT = 1e-3
maxDeltaT = 1e12
atol = 1e-8
timeIntInds = [psi slip]
#timeIntInds = [slip gVxy gVxz]
#timeIntInds = [gVxy gVxz]


