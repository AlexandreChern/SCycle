#outputDir = /scratch/kallison/versions/v2_
#outputDir = /scratch/kallison/versions/v3_
#outputDir = /scratch/kallison/versions/v4_
#outputDir = /scratch/kallison/versions/v5_
#outputDir = /scratch/kallison/versions/v6_
#outputDir = /scratch/kallison/versions/v7_

#outputDir = /scratch/kallison/versions/v8_
#outputDir = /scratch/kallison/versions/v9_
#outputDir = /scratch/kallison/versions/v10_



outputDir = data/v0_






