#outputDir = /scratch/kallison/agu_ss3/ssg25_Tss_cs_v09_lambda8_
#outputDir = /scratch/kallison/agu_ss3/ssg25_Tss_cs_v09_lambda7_Ny301_Nz301_
#outputDir = /scratch/kallison/agu_ss3/ssg25_Tss_cs_v09_lambda7_a6_

# g = 30
#outputDir = /scratch/kallison/agu_ss3/ssg30_Tss_cs_v09_lambda8_Ny301_Nz301_
#outputDir = /scratch/kallison/agu_ss3/ssg30_Tss_cs_v09_lambda7_Ny251_Nz301_

